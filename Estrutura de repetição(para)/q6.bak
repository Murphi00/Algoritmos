programa
{
	
	funcao inicio()
	{
		inteiro inicial, qtd, cont = 0, i
		cadeia nome
		
		escreva("Digite seu nome:	")
		leia(nome)
		
		para(i = 0; i < 20; i++){
		escreva("\n",(i + 1), " - ", nome)
	}
}
